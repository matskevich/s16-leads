# telegram auth (заполни в .env)
TG_API_ID=
TG_API_HASH=

# S16 Project Settings (ключевые группы и настройки)
S16_SPACE_GROUP_ID=-1002188344480    # ID основной группы s16 space (референсная группа)  
S16_SPACE_GROUP_NAME=s16 space       # название основной группы для логов
S16_ENABLE_CROSS_CHECK=true          # включить сверку участников с s16 space
S16_MARK_EXISTING_MEMBERS=true       # помечать участников уже состоящих в s16 space
S16_EXPORT_COMPARISON=true           # экспортировать результаты сравнения

SESSION_NAME=s16_session        # можешь оставить так

# safety limits (можно менять)
RATE_RPS=4                      # rpc-запросов в секунду
MAX_DM_PER_DAY=20               # личных сообщений в сутки
MAX_JOINS_PER_DAY=20            # join/leave операций в сутки
MAX_GROUPS=200                  # максимум групп для аккаунта

# anti-spam advanced settings (опционально)
# RETRY_MAX_ATTEMPTS=3          # максимум retry при FLOOD_WAIT
# SMART_PAUSE_PARTICIPANTS=5000 # пауза каждые N участников
# SMART_PAUSE_DM_BATCH=20       # пауза каждые N DM

# logging (опционально)
LOG_LEVEL=INFO                  # уровень логирования
SAFE_LOG_ENABLED=true           # включить anti-spam логи

# data directories (можно менять)
DATA_DIR=data
SESSIONS_DIR=data/sessions
EXPORT_DIR=data/export
ANTI_SPAM_DIR=data/anti_spam
LOGS_DIR=data/logs

# security settings (опционально)
SESSION_PERMISSIONS=600         # права доступа к сессиям
ENABLE_2FA_CHECK=true           # проверка 2FA
CHECK_USERNAME=true             # проверка наличия username
CHECK_PROFILE_PHOTO=true        # проверка аватара
CHECK_BIO=true                  # проверка био
WARMUP_PERIOD_HOURS=24          # период прогрева новых SIM

# development settings (для разработки)
DEBUG_MODE=false
TEST_MODE=false
DRY_RUN=false

# (опц.) сетевые настройки
# PROXY_URL=socks5://user:pass@host:port
